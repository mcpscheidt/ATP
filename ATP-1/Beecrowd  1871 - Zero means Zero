#include <stdio.h>
#include <string.h>

int main()
{
    int numero1, numero2, soma, i, j;
    char num[20];

    while (1) {
        scanf("%d %d", &numero1, &numero2);

        if (numero1 == 0 && numero2 == 0) {
            break; // encerra quando ambos s√£o zero
        }

        soma = numero1 + numero2;
        sprintf(num, "%d", soma);

        // Remove zeros
        j = 0;
        for (i = 0; num[i] != '\0'; i++) {
            if (num[i] != '0') {
                num[j++] = num[i];
            }
        }
        num[j] = '\0';

        printf("%s\n", num);
    }

    return 0;
}
